<h3>Archive</h3>
<ul>
    {% for key, value in articles %}
        {#<pre>{{ dump(articles) }}</pre>#}
        <li>{{ key }} ({{ value.count }})
            <ul>
                {% for item in value.items  %}
                    {% for month, cpt in item %}
                        <li><a href="{{ path('article_list_filter', {'month': month, 'year': key}) }}">{{ ("01-" ~ month ~ "-2016")|localizeddate('medium', 'none', null, 'Europe/Paris', 'MMMM') }} ({{ cpt }})</a></li>
                    {% endfor %}
                {% endfor %}
            </ul>
        </li>
    {% endfor %}
</ul>